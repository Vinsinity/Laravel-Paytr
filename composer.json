{
    "name": "softliya/paytr",
    "description": "Laravel PayTR POS Library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Softliya\\Paytr\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/collections": "^8|^9|^10",
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Softliya\\Paytr\\PayTRServiceProvider"
            ],
            "aliases": {
                "PayTR": "Softliya\\Paytr\\PayTRFacade"
            }
        }
    }
}
